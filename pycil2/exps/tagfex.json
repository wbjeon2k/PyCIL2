{
    "prefix": "reproduce",
    "dataset": "cifar100_aa",
    "memory_size": 2000,
    "memory_per_class": 20,
    "fixed_memory": false,
    "shuffle": true,
    "init_cls": 10,
    "increment": 10,
    "model_name": "tagfex",
    "convnet_type": "resnet18",
    "device": ["0"],
    "seed": [1993],
    "init_interpolation_factor": 0.95,
    "infonce_temp":0.2,
    "kd_temp":2,
    "infonce_kd_temp":0.2,
    "ta_convnet_type":"resnet18",
    "proj_hidden_dim":2048,
    "proj_output_dim":1024,
    "attn_num_heads":8,
    "contrast_factor":1,
    "trans_cls_factor":1,
    "transfer_factor":1,
    "aux_factor":2,
    "contrast_kd_factor":2,
    "aug":2,
    "init_epoch": 200,
    "init_lr": 0.1,
    "init_milestones": [60, 120, 170],
    "init_lr_decay": 0.1,
    "init_weight_decay": 0.0005,
    "momentum": 0.9,
    "epochs": 170,
    "lrate": 0.1,
    "milestones": [80, 120, 150],
    "lrate_decay": 0.1,
    "batch_size": 128,
    "weight_decay": 0.0002,
    "num_workers": 8,
    "T": 2,
    "eval_freq": 5
}